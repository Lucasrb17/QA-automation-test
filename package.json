{
	"name": "@zala-team/panel-web",
	"description": "Panel SPA",
	"version": "1.74.8",
	"private": true,
	"author": "Gabriel Villoldo <gabrielvilloldo@gmail.com>",
	"type": "module",
	"sideEffects": false,
	"imports": {
		"#panel-playwright/*": "./playwright/*.ts"
	},
	"scripts": {
		"dev": "run-p dev:vite dev-api-alive",
		"dev:vite": "vite",
		"dev:zrok": "zrok share public https://dashboard-local.zala.app:4002/",
		"dev-api-alive": "run-script-os",
		"dev-api-alive:win32": "cli\\.dev-api-alive.cmd",
		"dev-api-alive:darwin:linux": "sh ./cli/.dev-api-alive.sh",
		"build": "vite build",
		"test": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"generate-pwa-assets": "pwa-assets-generator",
		"playwright:install:deps": "pnpm exec playwright install --with-deps",
		"test:playwright": "pnpm exec playwright test",
		"test:playwright:codegen": "pnpm exec playwright codegen",
		"test:playwright:ui": "pnpm exec playwright test --ui",
		"typecheck": "tsc -b"
	},
	"engines": {
		"node": ">=22.x"
	},
